<div class="identify-container">
    <div class="body-popup-identify">
        <div class="identify-image-container">
            <center>
                {{#if images}}
                {{#each images}}
                {{#if @index}}
                <a class="fancybox-link" data-fancybox="gallery-{{this.id}}" href="{{this.url}}">
                    <img src="{{this.url}}" class="identify-fancybox" alt="" style="display:none" />
                </a>
                {{else}}
                <a class="fancybox-link" data-fancybox="gallery-{{this.id}}" href="{{this.url}}">
                    <img src="{{this.url}}" class="identify-fancybox" alt="" />
                </a>
                {{/if}}
                {{/each}}
                {{else}}
                <img src="data:image/png;base64," alt="" />
                {{/if}}
            </center>
        </div>
        <table class="table table-bordered table-striped" style="max-height: 300px;overflow-x:auto;display:block;">
            <tbody>
                {{#each views}}
                <tr>
                    <td width="30%">
                        <b>{{@key}}</b>
                    </td>
                    <td>
                        {{this}}
                    </td>
                </tr>
                {{/each}}
                {{!-- <tr>
                    <td colspan="2">
                        <strong>Tài liệu liên quan</strong>
                    </td>
                </tr>
                {{#each files}}
                <tr>
                    <td colspan="2">
                        <a href="{{this.url}}" target="_blank">{{this.file_name}}</a>
                    </td>
                </tr>
                {{/each}} --}}
            </tbody>
        </table>
    </div>
    <!--<div class="footer-popup-identify">
        <a id="identify-toggle">Xem chi tiết</a>
    </div>-->
</div>