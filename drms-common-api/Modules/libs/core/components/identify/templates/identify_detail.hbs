<div class="detail-header">
    <span>{{layer_name}}</span>
    {{!-- <a href="javascript:;" id="notifyBtn">
        Gửi thông báo
    </a> --}}
    {{#canUpdate ""}}
    <a href="javascript:;" id="editFeature">
        Chỉnh sửa
    </a>
    {{/canUpdate}}
    {{#canCreate ""}}
    <a href="javascript:;" id="deleteFeature">
        Xóa
    </a>
    {{/canCreate}}
    {{#if isPolygon}}
     <a href="javascript:;" id="polygonFilter">
        Tra cứu đổi tượng trong vùng
    </a>
    {{/if}}
</div>
<nav class="navigations">
    <ul class="nav nav-tabs" id="tabs-result" role="tablist">
        <li class="nav-item" role="presentation">
            <a href="#attributes-content" class="active" data-toggle="tab" data-bs-target="#attributes-content" role="tab">Thông tin</a>
        </li>
        {{!-- <li class="nav-item" role="presentation">
            <a id="document-title" href="#document-content" data-toggle="tab" data-bs-target="#document-content" role="tab">Hồ sơ</a>
        </li>
        <li class="nav-item" role="presentation">
            <a id="plan-title" href="#plan-content" data-toggle="tab" data-bs-target="#plan-content" role="tab">Kế hoạch</a>
        </li>
        <li class="nav-item" role="presentation">
            <a id="maintenance-title" href="#maintenance-content" data-toggle="tab" data-bs-target="#maintenance-content" role="tab">Bảo dưỡng</a>
        </li> --}}
    </ul>
</nav>
<div class="tab-content">
    <div class="tab-pane fade show active" id="attributes-content" role="tabpanel">
        <div class="identify-image-container">
            <center class="h-100">
                {{#if images}}
                {{#each images}}
                {{#if @index}}
                <a class="fancybox-link" data-fancybox="gallery" href="{{path}}" data-id="{{id}}" data-path="{{path}}" data-name="{{file_name}}">
                    <img src="{{path}}" class="identify-fancybox" alt="" style="display:none" />
                </a>
                {{else}}
                <a class="fancybox-link" data-fancybox="gallery" href="{{path}}" data-id="{{id}}" data-path="{{path}}" data-name="{{file_name}}">
                    <img src="{{path}}" class="identify-fancybox" alt="" />
                </a>
                {{/if}}
                {{/each}}
                {{else}}
                <img src="data:image/png;base64," alt="" />
                {{/if}}
            </center>
        </div>
        <table class="table table-bordered" style="max-height: 300px;overflow-x:auto" id="attribute-table">
            <tbody>
                {{#each views}}
                <tr>
                    <td width="30%">
                        <b>{{@key}}</b>
                        {{!-- {{#if has_child_data}}
                        <a href="javascript:;" class="view-detail-relation" data-id={{column.data_in_radius_of_layer}}>
                            <b>{{@key}}</b>
                        </a>
                        {{else}}
                        <b>{{@key}}</b>
                        {{/if}} --}}
                    </td>
                    <td>
                        {{#if has_child_data}}
                        <a href="javascript:;" class="view-detail-relation" data-id={{column.data_in_radius_of_layer}}>
                            Xem thông tin
                            {{!-- <b>({{count_data}}) {{column.name_vn}}</b> --}}
                        </a>
                        {{else}}
                        {{#if has_data}}
                        {{#if is_link}}
                        <p>
                            <a href="{{value}}" target="_blank" class="text-success font-weight-bold">
                                <i class="icon icon-eye"></i> Xem
                            </a>
                        </p>
                        <span>
                            <a href="{{value}}" class="text-danger font-weight-bold">
                                <i class="icon icon-document-download"></i> Tải về
                            </a>
                        </span>
                        {{else}}
                        {{value}}
                        {{/if}}
                        {{else}}
                        {{#if is_column_code}}
                        <span style="font-style: italic;"></span>
                        {{else}}
                        <span style="font-style: italic;"></span>
                        {{/if}}
                        {{/if}}
                        {{/if}}
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    <div class="tab-pane fade feature-tab-content" id="document-content" role="tabpanel">
        {{!-- {{#if files.length}}
        <a href="javascript:;" class="identify-detail-btn feature-document-detail">Chi tiết</a>
        {{/if}} --}}
        <table class="table table-bordered" style="max-height: 300px;overflow-x:auto" id="feature-document-table">
            <thead>
                <tr>
                    <th class="text-center" scope="col" width="50">STT</th>
                    <th class="text-center" scope="col" width="300">Tên hồ sơ</th>
                    <th class="text-center" scope="col" width="50"></th>
                </tr>
            </thead>
            <tbody>
                {{#if documents.length}}
                {{#each documents}}
                <tr>
                    <td class="text-center" scope="col" width="50">{{counter}}</td>
                    <td class="text-left" scope="col" width="300">
                        <a class="view-document" data-url="{{path}}">
                            {{file_name}}
                        </a>
                    </td>
                    <td class="text-center" scope="col" width="100">
                        <a href="{{path}}" target="_blank">
                            Tải xuống
                        </a>
                    </td>
                </tr>
                {{/each}}
                {{else}}
                <tr>
                    <td class="text-center" colspan="2">Không có dữ liệu!</td>
                </tr>
                {{/if}}
            </tbody>
        </table>
    </div>
    <div class="tab-pane fade feature-tab-content" id="plan-content" role="tabpanel">
        {{#if plans.length}}
        <a href="javascript:;" class="identify-detail-btn" id="viewMaintenancePlans">Chi tiết</a>
        {{/if}}
        <table class="table table-bordered" style="max-height: 300px;overflow-x:auto" id="feature-plan-table">
            <thead>
                <tr>
                    <th class="text-center" scope="col" width="150">Tên kế hoạch</th>
                    <th class="text-center" scope="col" width="130">Địa điểm</th>
                    <th class="text-center" scope="col" width="120">Ngày bắt đầu</th>
                </tr>
            </thead>
            <tbody>
                {{#if plans.length}}
                {{#each plans}}
                <tr>
                    <td class="text-left" scope="col" width="150">{{tenkehoach}}</td>
                    <td class="text-left" scope="col" width="130">{{diadiem}}</td>
                    <td class="text-center" scope="col" width="120">{{toLocalDateString ngaybatdau}}</td>
                </tr>
                {{/each}}
                {{else}}
                <tr>
                    <td class="text-center" colspan="5">Không có dữ liệu!</td>
                </tr>
                {{/if}}
            </tbody>
        </table>
    </div>
    <div class="tab-pane fade feature-tab-content" id="maintenance-content" role="tabpanel">
        {{#if maintenances.length}}
        <a href="javascript:;" class="identify-detail-btn" id="viewMaintenances">Chi tiết</a>
        {{/if}}
        <table class="table table-bordered" style="max-height: 300px;overflow-x:auto" id="feature-maintenance-table">
            <thead>
                <tr>
                    <th class="text-center" scope="col" width="150">Phương thức</th>
                    <th class="text-center" scope="col" width="150">Công cụ</th>
                    <th class="text-center" scope="col" width="100">Thiết bị</th>
                </tr>
            </thead>
            <tbody>
                {{#if maintenances.length}}
                {{#each maintenances}}
                <tr>
                    <td class="text-left" scope="col" width="150">{{phuongthuckiemtra}}</td>
                    <td class="text-left" scope="col" width="150">{{congcukiemtra}}</td>
                    <td class="text-left" scope="col" width="100">{{diadiem}}</td>
                </tr>
                {{/each}}
                {{else}}
                <tr>
                    <td class="text-center" colspan="5">Không có dữ liệu!</td>
                </tr>
                {{/if}}
            </tbody>
        </table>
    </div>
</div>