{{#if this}}
{{#each this}}
<li class="management-item">
    <a href="{{url}}">
        <div class="management-box">
            <div class="management-image">
                <img src="{{icon}}" alt="" width="100px" height="100px">
            </div>
            <div class="management-body">
                <p class="mb-0">{{name}}</p>
            </div>
        </div>
    </a>
</li>
{{/each}}
{{/if}}