{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "target": "es2017",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "paths": {},
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules/", "build/", "public/", "wwwroot"]
}
